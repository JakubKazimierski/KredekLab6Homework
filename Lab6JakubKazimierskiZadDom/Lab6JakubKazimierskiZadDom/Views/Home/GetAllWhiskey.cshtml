@model List<WhiskeyViewModel>
@{
    ViewData["Title"] = "GetAllWhiskey";
}

<h1>GetAllWhiskey</h1>
<!--div with item for sell -->
@foreach (var whiskey in Model)
{
    <div>
        <hr />
        <h2>@whiskey.Model</h2>
        <h4>Producer: @whiskey.Producer</h4>
        <h4>Price: @whiskey.Price zł</h4>
        <img src="@Url.Content(whiskey.Photo)" alt="@whiskey.Model" />
        <hr />

    </div>
}

