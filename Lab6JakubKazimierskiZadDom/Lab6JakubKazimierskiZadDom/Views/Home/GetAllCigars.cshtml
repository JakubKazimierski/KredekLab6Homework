@model List<CigarsViewModel>
@{
    ViewData["Title"] = "GetAllCigars";
}

<h1>GetAllCigars</h1>
@foreach (var cigar in Model)
{
    <div>
        <hr />
        <h2>@cigar.Model</h2>
        <h4>Producer: @cigar.Producer</h4>
        <h4>Price: @cigar.Price zł</h4>
        <img src="@Url.Content(cigar.Photo)" alt="@cigar.Model" />
        <hr />

    </div>
}
